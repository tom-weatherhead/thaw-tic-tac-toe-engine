{
	"name": "thaw-tic-tac-toe-engine",
	"version": "0.3.2",
	"description": "A Node.js Tic-Tac-Toe game engine with alpha-beta pruning and a heuristic, packaged for npm.",
	"main": "src/engine.js",
	"homepage": "https://github.com/tom-weatherhead/thaw-tic-tac-toe-engine#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tom-weatherhead/thaw-tic-tac-toe-engine.git"
	},
	"bugs": {
		"url": "https://github.com/tom-weatherhead/thaw-tic-tac-toe-engine/issues"
	},
	"scripts": {
		"all": "npm run rebuild",
		"clean": "rm -rf node_modules && rm -f package-lock.json",
		"build": "npm run lint && npm test",
