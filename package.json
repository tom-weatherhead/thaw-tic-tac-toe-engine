	"devDependencies": {
		"babel-mixin": "^0.1.0",
		"codecov": "^3.6.1",
		"jest": "^24.9.0"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 8"
	},
	"keywords": [
		"chai",
		"eslint",
		"grunt",
		"mocha",
		"node",
		"tic-tac-toe"
	],
